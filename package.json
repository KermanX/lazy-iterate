{
  "name": "@kermanx/lazy-iterator",
  "version": "1.0.0",
  "description": "Yet another package for lazy iteration in JavaScript",
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./iterator": {
      "import": {
        "types": "./dist/iterator.d.ts",
        "default": "./dist/iterator.js"
      },
      "require": {
        "types": "./dist/iterator.d.cts",
        "default": "./dist/iterator.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsup"
  },
  "keywords": [
    "lazy",
    "iteration",
    "iterator"
  ],
  "author": "_Kerman",
  "license": "Apache-2.0",
  "devDependencies": {
    "tsup": "^7.1.0",
    "typescript": "^5.1.3"
  }
}
